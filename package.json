{
  "name": "react-native-lib-builder-bob-story-2",
  "version": "0.6.0",
  "description": "biblioteca-builder-with-story-2",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-lib-builder-bob-story-2-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it",
    "storybook": "cross-env BABEL_ENV=development storybook dev -p 6006",
    "build-storybook": "cross-env BABEL_ENV=development storybook build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vagnerzadoque/react-native-lib-builder-bob-story-2.git"
  },
  "author": "Vagner Barbosa <vagner.zaham@gmail.com> (https://github.com/vagnerzadoque)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vagnerzadoque/react-native-lib-builder-bob-story-2/issues"
  },
  "homepage": "https://github.com/vagnerzadoque/react-native-lib-builder-bob-story-2#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-flow": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/runtime": "^7.26.0",
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/babel-preset": "^0.76.2",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^5.0.0",
    "@storybook/addon-actions": "^8.3.6",
    "@storybook/addon-console": "^3.0.0",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.3.12",
    "babel-jest": "^29.7.0",
    "commitlint": "^17.0.2",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-native": "0.74.5",
    "react-native-builder-bob": "^0.30.2",
    "release-it": "^15.0.0",
    "typescript": "^5.6.3"
  },
  "optionalDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@storybook/addon-essentials": "^8.3.3",
    "@storybook/addon-interactions": "^8.3.3",
    "@storybook/addon-links": "^8.3.3",
    "@storybook/addon-onboarding": "^8.3.3",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.3.3",
    "@storybook/react": "^8.3.3",
    "@storybook/react-webpack5": "^8.3.3",
    "@storybook/test": "^8.3.3",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.3.3"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier",
      "plugin:storybook/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "library",
    "version": "0.41.2"
  },
  "dependencies": {
    "@naturacosmeticos/natds-icons": "^1.26.5",
    "@naturacosmeticos/natds-themes": "^0.80.1",
    "@storybook/addon-styling": "^1.3.7",
    "react-native-web": "^0.19.12",
    "styled-components": "^6.1.13"
  }
}
